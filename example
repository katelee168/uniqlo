																																										} catch (error) {
																																														return false;
																																																	}
																																																				}
																																																						};

																																																							var first_name = "";

																																																							    if (cust_name.checkIfLocalStorageNameSupported()) {
																																																														var loginCookie = JSON.parse(sessionStorage.getItem('clogin'));
